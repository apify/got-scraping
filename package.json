    "description": "HTTP client made for scraping based on got.",
    "engines": {
        "node": ">=16"
    },
    "files": [
        "dist"
    ],
    "exports": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
    },
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "type": "module",
    "dependencies": {
        "got": "^13.0.0",
        "header-generator": "^2.1.41",
        "http2-wrapper": "^2.2.0",
